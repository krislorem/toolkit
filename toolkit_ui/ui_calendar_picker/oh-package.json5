  "author": "krislorem",
  "license": "ISC",
  "keywords": [
    "日历组件",
    "Calendar",
    "DatePicker",
    "日期选择",
    "范围选择",
    "多选",
    "时间选择",
    "禁选",
    "滑动",
  ],
  "dependencies": {
    "@krislorem/toolkit_core": "^1.0.1",
    "@krislorem/ui-base": "^1.0.0",
  }
}