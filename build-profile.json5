{
  "app": {
    "signingConfigs": [],
    "products": [
      {
        "name": "default",
        "signingConfig": "default",
        "targetSdkVersion": "6.0.0(20)",
        "compatibleSdkVersion": "6.0.0(20)",
        "runtimeOS": "HarmonyOS",
        "buildOption": {
          "strictMode": {
            "caseSensitiveCheck": true,
            "useNormalizedOHMUrl": true
          }
        }
      }
    ],
    "buildModeSet": [
      {
        "name": "debug",
      },
      {
        "name": "release"
      }
    ]
  },
  "modules": [
    {
      "name": "entry",
      "srcPath": "./entry",
      "targets": [
        {
          "name": "default",
          "applyToProducts": [
            "default"
          ]
        }
      ]
    },
    {
      "name": "toolkit_core",
      "srcPath": "./toolkit_core",
    },
    {
      "name": "ui_base",
      "srcPath": "./toolkit_ui/ui_base"
    },
    {
      "name": "ui_swiper",
      "srcPath": "./toolkit_ui/ui_swiper",
    },
    {
      "name": "ui_tag",
      "srcPath": "./toolkit_ui/ui_tag",
    }
  ]
}